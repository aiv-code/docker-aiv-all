{
    "dremio": {
        "month": "DATE_PART('MONTH', ${column})",
        "year": "DATE_PART('YEAR', ${column})",
        "week": "DATE_PART('WEEK', ${column})",
		"limitation": "LIMIT ${limit} OFFSET ${offset}"
    },
	"bigquery": {
        "month": "MONTH(${column})",
        "year": "YEAR(${column})",
        "week": "DATEPART(WEEK, ${column})",
		"limitation": "LIMIT ${limit} OFFSET ${offset}"
    },
    "mysql": {
        "month": "MONTH(${column})",
        "month_name": "MONTHNAME(${column})",
        "year": "YEAR(${column})",
        "week": "WEEK(${column})",
        "weekday": "WEEKDAY(${column})",
        "day": "DAY(${column})",
        "day_name": "DAYNAME(${column})",
        "day_of_week": "DAYOFWEEK(${column})",
        "day_of_year": "DAYOFYEAR(${column})",
        "to_days": "TO_DAYS(${column})",
        "time": "TIME(${column})",
        "hour": "HOUR(${column})",
        "minute": "MINUTE(${column})",
        "second": "SECOND(${column})",
        "to_seconds": "TO_SECONDS(${column})",
        "last_day_of_month": "LAST_DAY(${column})",
        "quarter": "QUARTER(${column})",
        "timestamp": "TIMESTAMP(${column})",
        "unix_timestamp": "UNIX_TIMESTAMP(${column})",
        "from_unixtime": "FROM_UNIXTIME(${column})",
		"limitation": "LIMIT ${limit} OFFSET ${offset}"
    },
    "postgresql": {
        "age": "AGE(${column}::timestamp)",
        "month": "EXTRACT(MONTH FROM ${column})",
        "month_name": "TO_CHAR(${column}, 'Month')",
        "year": "EXTRACT(YEAR FROM ${column})",
        "week": "EXTRACT(WEEK FROM ${column})",
        "weekday": "EXTRACT(ISODOW FROM ${column})",
        "day": "EXTRACT(DAY FROM ${column})",
        "day_name": "TO_CHAR(${column}, 'Day')",
        "day_of_week": "EXTRACT(DOW FROM ${column})",
        "day_of_year": "EXTRACT(DOY FROM ${column})",
        "time": "EXTRACT(TIMEZONE FROM ${column})",
        "hour": "EXTRACT(TIMEZONE_HOUR FROM ${column})",
        "minute": "EXTRACT(TIMEZONE_MINUTE FROM ${column})",
        "second": "EXTRACT(SECOND FROM ${column})",
        "quarter": "EXTRACT(QUARTER FROM ${column})",
        "to_unix_timestamp": "EXTRACT(EPOCH FROM ${column}::TIMESTAMP)",
		"limitation": "LIMIT ${limit} OFFSET ${offset}",
	"startsep":"\"",
		"endsep":"\""
    },
    "mssql": {
        "month": "MONTH(${column})",
        "month_name": "DATENAME(MONTH,${column})",
        "year": "YEAR(${column})",
        "day": "DAY(${column})",
        "week": "DATEPART(WEEK, ${column})",
        "week_name": "DATENAME(WEEKDAY, ${column})",
        "weekday": "DATEPART(WEEKDAY, ${column})",
        "day_of_year": "DATEPART(DAYOFYEAR, ${column})",
        "hour": "DATEPART(HOUR, ${column})",
        "minute": "DATEPART(MINUTE, ${column})",
        "second": "DATEPART(SECOND, ${column})",
        "quarter": "DATEPART(QUARTER, ${column})",
        "time": "CAST(${column} AS TIME)",
		"limitation": "row_num BETWEEN ${offset} AND ${limit}"
    },
    "oracle": {
        "month": "TO_NUMBER(TO_CHAR(${column}, 'MM'))",
        "month_name": "TO_CHAR(${column}, 'MONTH')",
        "year": "TO_NUMBER(TO_CHAR(${column}, 'YYYY'))",
        "week": "TO_NUMBER(TO_CHAR(${column}, 'WW'))",
        "weekday": "TO_NUMBER(TO_CHAR(${column}, 'D'))",
        "day": "TO_NUMBER(TO_CHAR(${column}, 'DD'))",
        "day_name": "TO_CHAR(${column}, 'DAY')",
        "day_of_year": "TO_NUMBER(TO_CHAR(${column}, 'DDD'))",
        "hour": "TO_NUMBER(TO_CHAR(${column}, 'HH24'))",
        "minute": "TO_NUMBER(TO_CHAR(${column}, 'MI'))",
        "second": "TO_NUMBER(TO_CHAR(${column}, 'SS'))",
        "last_day_of_month": "LAST_DAY(${column})",
        "quarter": "TO_NUMBER(TO_CHAR(${column}, 'Q'))",
        "time": "TO_CHAR(${column}, 'HH24:MI:SS')",
		"limitation": "OFFSET ${offset} ROWS FETCH NEXT ${limit} ROWS ONLY"
    }
}